{
  "name": "eshop-order-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "eshop-order-e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nx/playwright:playwright",
      "outputs": ["{workspaceRoot}/dist/.playwright/eshop-order-e2e"],
      "options": {
        "config": "eshop-order-e2e/playwright.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["eshop-order-e2e/**/*.{js,ts}"]
      }
    }
  },
  "implicitDependencies": ["eshop-order"]
}
